        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:metroui="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
                     xmlns:sys="clr-namespace:System;assembly=mscorlib"
                     Title="BTOC" WindowStartupLocation="CenterScreen" ShowIconOnTitleBar="False" Height="500" Width="800" ShowInTaskbar="True" WindowState="Maximized" >
    <Window.Resources>
        <ResourceDictionary Source="Style\Skin.xaml"></ResourceDictionary>
    </Window.Resources>
    <metroui:MetroWindow.RightWindowCommands>
        <metroui:WindowCommands>
            <StackPanel Orientation="Horizontal">
                <Image Name="ImgUser" Height="20" Width="20" Stretch="Fill"/>
                <TextBlock Name="TbUserName" Style="{StaticResource PropertyNameMediumStyle}" Margin="5,0,0,0" Height="20" Width="auto" />
                <TextBlock Name="TbUserRole" Style="{StaticResource PropertyNameMediumStyle}"   Height="20" Width ="auto" Margin="5,0,0,0"  Text="System"/>
                <Separator Style="{StaticResource VerticalSeparatorStyle}"></Separator>
                <TextBlock Name="TbCurrentDateTime" Style="{StaticResource PropertyNameMediumStyle}" Height="20" Width ="Auto"  Margin="5,0,0,0"/>
                <Separator Style="{StaticResource VerticalSeparatorStyle}"></Separator>
            </StackPanel>
        </metroui:WindowCommands>
    </metroui:MetroWindow.RightWindowCommands>
